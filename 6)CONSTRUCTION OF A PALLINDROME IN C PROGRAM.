#include<stdio.h>
#include<conio.h>
main()
{
int n,reversed=0,remainder,original;
clrscr();
printf("enter an interger=");
scanf("%d",&n);
original=n;
while(n!=0)
{
remainder=n%10;
reversed=reversed*10+remainder;
n/=10;
}
if(original==reversed)
printf("this is a pallindrome",original);
else
printf("this is not a pallindrome",original);
getch();
return 0;
}
